# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal
components/**/db/*.sqlite3

# Ignore all logfiles and tempfiles.
/log/*
components/**/log/*
!/log/.keep
/tmp
/components/**/tmp

# Ignore vim swapfiles
*.swp

*.keep

# Ignore local data content
db/data
