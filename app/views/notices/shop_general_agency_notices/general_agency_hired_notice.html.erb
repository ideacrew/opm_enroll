<%= render partial: 'notices/shared/cover_page', :locals => {notice: notice} %>
<%= render partial: 'notices/shared/logo_and_address_shop' %>
<%= render partial: "notices/shared/shop_address", :locals => {notice: notice} %>

<table class="total_table" border="0">
  <tbody>
    <tr>
      <td style="padding-top: 10px"><strong>Email notification sent to:</strong> <%= notice.general_agent_email %></h3></td>
    </tr>
  </tbody>
</table>

<%= render partial: "notices/shared/date", :locals => {notice: notice} %>

<table class="total_table" border="0">
  <tbody>
    <tr>
      <td style="padding-top: 10px"><strong><%= notice.general_agency.employer.upcase %> HAS HIRED YOU AS A GENERAL AGENCY</strong></td>
    </tr>
  </tbody>
</table>

<table class="total_table" border="0">
  <tbody>
    <tr>
      <td style="padding-top: 10px">Dear <strong><%= notice.primary_fullname %><strong>,</td>
    </tr>
  </tbody>
</table>

<%= render partial: 'notices/shared/paragraph', locals: {
 content: "Congratulations! You have been selected as the General Agency for <strong>#{notice.general_agency.employer}</strong> as of <strong>#{notice.general_agency.general_agency_account_start_on}</strong>. The broker for this <strong>#{notice.general_agency.employer}</strong> is <strong>#{notice.general_agency.broker_fullname}</strong> and the primary contact for the group is <strong>#{notice.general_agency.employer_fullname}</strong>."} %>

 <%= render partial: 'notices/shared/paragraph', locals: {
 content: "If you need resources in assisting your new client, please look to <a href=https://dchealthlink.com/forms>DCHealthLink.com/forms</a>."} %>
 <br>
 
<%= render partial: 'notices/shared/paragraph_with_headers', :locals =>
  { contents: [""], heading: 'For Questions or Assistance:', sub_heading: ''} %>

<%= render partial: 'notices/shared/paragraph', locals: {
 content: "Please contact #{Settings.site.short_name} with any questions:"} %>

<%= render partial: 'notices/shared/unordered_list', :locals => { listings:
  ["By calling #{notice.hbe.phone}",
    "TTY: #{Settings.contact_center.tty}",
    "Online at <a href=#{Settings.site.home_url}>#{Settings.site.website_name}</a>"]} %>
