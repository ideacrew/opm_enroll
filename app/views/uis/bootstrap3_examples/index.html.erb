    <h3 id="buttons"><%= l10n('.button_classes') %></h3>

    <!-- Standard button -->
    <button type="button" class="btn btn-default"><%= l10n('.default') %></button>

    <!-- Provides extra visual weight and identifies the primary action in a set of buttons -->
    <button type="button" class="btn btn-primary"><%= l10n('.primary') %></button>

    <!-- Indicates a successful or positive action -->
    <button type="button" class="btn btn-success"><%= l10n('.success') %></button>

    <!-- Contextual button for informational alert messages -->
    <button type="button" class="btn btn-info"><%= l10n('.info') %></button>

    <!-- Indicates caution should be taken with this action -->
    <button type="button" class="btn btn-warning"><%= l10n('.warning') %></button>

    <!-- Indicates a dangerous or potentially negative action -->
    <button type="button" class="btn btn-danger"><%= l10n('.danger') %></button>

    <!-- Deemphasize a button by making it look like a link while maintaining button behavior -->
    <button type="button" class="btn btn-link"><%= l10n('.link') %></button>

