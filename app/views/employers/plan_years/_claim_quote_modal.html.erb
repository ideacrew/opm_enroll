<div class="modal claimQuoteModal" id="claimQuoteModal" tabindex="-1" role="dialog" role="document">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title">Create Plan Year by claiming a broker quote</h4>
      </div>
      <div class="modal-body">
        <p>
          Please enter your claim code to create a Plan Year and Benefit Package based on the quote provided by your broker.
        </p>
        <!-- form_for @employer_profile, url: {action: "link_from_quote"}, method: "get", class: "form-inline" do |f|  -->
        <%= form_for :employer_profile, url: sponsored_benefits.organizations_plan_design_proposal_claim_path(@employer_profile), method: "get", class: "form-inline" do |f| %>
        <label><%= text_field_tag :claim_code, nil, placeholder: 'X X X X - X X X X', maxlength: 9 %></label>
      </br></br>
      <!--   <p><label><%= check_box_tag "import_roster", "yes", false %> Import roster</label></p>
      </br> -->
        <%= submit_tag "Claim Code", class: 'btn btn-primary' %>
        <%end%>


        <ul class="application-warnings"></ul>
      </div>
      <div class="modal-footer">
        <%= link_to 'Cancel', "", class: 'btn btn-default', data: {'dismiss': 'modal' } %>
      </div>
    </div>
  </div>
</div>
